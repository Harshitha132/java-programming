import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    int a=sc.nextInt();
    int count=1;
        for(int i=1;i<=a;i++){
    int n=a%10;
        count=count+1;
    a=a/10;
        }
     System.out.println("The Count the digits: "+count);   
    }
}
