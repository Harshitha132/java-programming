import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        char c=sc.next().charAt(0);
        float d,e,f,g,h;
    
        switch(c){
         case '+':
                d=a+b;
                System.out.println("Addition of two number is "+d);
                break;
        
         case '-':
                e=a-b;
                System.out.println("Subtraction of two number is "+e);
        break;
        
         case '*':
                f=a*b;
                System.out.println("Multiplication of two number is "+f);
       break;
        
         case '/':
                g=a/b;
                System.out.println("Division of two number is "+g);
       break;
        
         case '%':
                h=a%b;
                System.out.println("Modulo of two number is "+h);
       break;
        default:
                System.out.println("Invalid Input");
        
                    
        }
    }
}
